{
  "name": "is-touch-device",
  "version": "0.0.0",
  "description": "Is the current JS environment a touch device?",
  "main": "build",
  "scripts": {
    "clean": "rimraf build && mkdirp src",
    "prebuild": "npm run clean",
    "build": "babel src --out-dir build",
    "prelint": "editorconfig-tools check *",
    "lint": "eslint .",
    "tests-only": "echo \"Error: no test specified\" && exit 1",
    "pretest": "npm run lint",
    "test": "npm run tests-only"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/airbnb/is-touch-device.git"
  },
  "keywords": [
    "touch",
    "is",
    "documenttouch",
    "maxtouchpoints",
    "msmaxtouchpoints",
    "touchpoints",
    "ontouchstart"
  ],
  "author": "Jordan Harband <ljharb@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/airbnb/is-touch-device/issues"
  },
  "homepage": "https://github.com/airbnb/is-touch-device#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-airbnb": "^2.2.3",
    "editorconfig-tools": "^0.1.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.3.0",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.6.1"
  }
}
